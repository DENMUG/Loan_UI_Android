<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Pay.cash.Advance.Insta">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.Pay.cash.Advance.Insta.StartActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.FirstActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.LoanDetailActivity"
            android:windowSoftInputMode="stateHidden|adjustPan"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.SignUpActivity"
            android:windowSoftInputMode="stateHidden|adjustPan"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.IdentificationDetailsActivity"
            android:windowSoftInputMode="stateHidden|adjustPan"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.LoanPurposeActivity"
            android:windowSoftInputMode="stateHidden|adjustPan"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.LoansDetails"
            android:windowSoftInputMode="stateHidden|adjustPan"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.LoanRequestActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.RateActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.SignInActivity"
            android:windowSoftInputMode="stateHidden|adjustPan"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.TermsActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.Pay.cash.Advance.Insta.ui.StatusCheckActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>